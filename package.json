{
  "name": "@fa-repo/undo-redo",
  "version": "0.0.3",
  "description": "A javascript package to handle basic undo/redo functionality",
  "author": "@fa-repo",
  "license": "mit",
  "repository": "@fa-repo/undo-redo",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "npx rollup -c",
    "start": "npx rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd examples && npm install && npm run production",
    "deploy": "npx gh-pages -d examples/public",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "prepublishOnly": "npm run build",
    "analyse": "npm run build && npx source-map-explorer ./dist/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
